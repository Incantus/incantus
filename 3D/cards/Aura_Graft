name = 'Aura Graft'
cardnum = 67
expansion = '10E'
type = characteristic('Instant')
supertype = characteristic('')
subtypes = characteristic([])
cost = '1U'
color = characteristic(['U'])
text = ["Gain control of target Aura that's attached to a permanent. Attach it to another permanent it can enchant."]

target = Target(target_types=isAura)
out_play_role.abilities = [CastNonPermanentSpell(card, cost,
				target=[target, Target(targeting=lambda t=target: t.target)],
				effects=[ChangeController(), ChangeAttachment(must_be_different=True)],
				copy_targets=False)
]



