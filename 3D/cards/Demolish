
name = 'Demolish'
cardnum = 196
expansion = '10E'
type = characteristic('Sorcery')
supertype = no_characteristic()
subtypes = no_characteristic()
cost = '3R'
color = characteristic(['R'])
text = ['Destroy target artifact or land.']

out_play_role.abilities = [CastSorcerySpell(card, cost,
				target=Target(target_types=[isArtifact, isLand]),
				effects=Destroy())
]
