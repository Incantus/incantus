name = 'Brainstorm'
cardnum = 61
expansion = 'Ice Age'
type = characteristic('Instant')
supertype = no_characteristic()
subtypes = no_characteristic()
cost = 'U'
color = characteristic(['U'])
text = ["Draw three cards, then put any two cards from your hand on top of your library."]


out_play_role.abilities = [CastNonPermanentSpell(card, cost,effects=[DrawCard(3),MoveCards(from_zone="hand", to_zone="library", to_position="top", number=2)])]
