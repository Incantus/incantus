name = 'Reminisce'
cardnum = 99
expansion = '10E'
type = characteristic('Sorcery')
supertype = no_characteristic()
subtypes = no_characteristic()
cost = '2U'
color = characteristic(['U'])
text = ['Target player shuffles his or her graveyard into his or her library.']

target=Target(target_types=isPlayer)
out_play_role.abilities = [CastSorcerySpell(card, cost,
                                target=target,
                                effects=[MoveCards(from_zone="graveyard", from_position="top", to_zone="library", number=-1), ShuffleLibrary()], copy_targets=False)
]