name = 'Wirewood Symbiote'
cardnum = 133
expansion = 'Scourge'
type = characteristic('Creature')
supertype = no_characteristic()
subtypes = characteristic(['Insect'])
cost = 'G'
color = characteristic(['G'])
text = ["Return an Elf you control to its owner's hand: Untap target creature. Play this ability only once each turn."]

out_play_role.abilities = [CastPermanentSpell(card, cost)]
subrole = Creature(1, 1)
in_play_role = Permanent(card, subrole)

#################################

subrole.abilities = [ActivatedAbility(
	card,
	cost=ReturnToHandCost(
		cardtype=isPermanent.with_condition(lambda c: c.subtypes == "Elf")),
	target = Target(target_types=isCreature),
	effects=UntapTargetPermanent(),
	limit=CountLimit(card, 1))]

#################################