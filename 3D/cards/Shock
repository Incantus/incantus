name = 'Shock'
cardnum = 232
expansion = '10E'
type = characteristic('Instant')
supertype = no_characteristic()
subtypes = no_characteristic()
cost = 'R'
color = characteristic(['R'])
text = ['Shock deals 2 damage to target creature or player.']

out_play_role.abilities = [CastInstantSpell(card, cost,
                                target=Target(target_types=[isCreature,isPlayer]),
                                effects=DealDamage(2))
]