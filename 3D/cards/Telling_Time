out_play_role.abilities = [CastNonPermanentSpell(card, cost, effects=[MoveCards(from_zone="library", to_zone="hand", number=1, subset=3), MoveCards(from_zone="library", to_zone="library", to_position="bottom", number=1, subset=2)])]



