name = 'Bloodrock Cyclops'
cardnum = 192
expansion = '10E'
type = characteristic('Creature')
supertype = no_characteristic()
subtypes = characteristic(['Cyclops'])
cost = '2R'
color = characteristic(['R'])
text = ['Bloodrock Cyclops attacks each turn if able.']

out_play_role.abilities = [CastPermanentSpell(card, cost)]
subrole = Creature(3, 3)

in_play_role = Permanent(card, subrole)

#################################

must_attack(subrole)

#################################