name = 'Commune with Nature'
cardnum = 256
expansion = '10E'
type = characteristic('Sorcery')
supertype = characteristic('')
subtypes = characteristic([])
cost = 'G'
color = characteristic(['G'])
text = ['Look at the top five cards of your library. You may reveal a creature card from among them and put it into your hand. Put the rest on the bottom of your library in any order.']

out_play_role.abilities = [CastNonPermanentSpell(card, cost, effects=MoveCards(from_zone="library", to_zone="hand", number=1, card_types=isCreatureType, subset=5, return_position="bottom", reveal=True))
]




