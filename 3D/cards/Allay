name = 'Allay'
cardnum = 1
expansion = 'Exodus'
type = characteristic('Instant')
supertype = no_characteristic()
subtypes = no_characteristic()
cost = '1W'
color = characteristic(['W'])
text = ['Buyback 3', 'Destroy target enchantment']

out_play_role.abilities = [CastNonPermanentSpell(card, cost,
				target=Target(target_types=isEnchantment),
				effects=Destroy())]

buyback(out_play_role, "3")






