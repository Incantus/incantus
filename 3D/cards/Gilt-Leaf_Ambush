name = "Gilt-Leaf Ambush"
cardnum = 214
expansion = 'Lorwyn'
type = characteristic('Instant')
supertype = no_characteristic()
subtypes = characteristic(['Elf'])
cost = '2G'
color = characteristic(['G'])
text = ["Put two 1/1 green Elf Warrior creature tokens into play", "Clash with an opponent. If you win, those creatures gain deathtouch until end of turn."]


# XXX Clash not implemented yet for non-permanents
out_play_role.abilities = [CastNonPermanentSpell(card, cost,
				target=Target(targeting="controller"),
				effects = CreateToken(number=2, token_info={"name": "Elf Warrior", "type": "Creature", "subtypes": ["Elf", "Warrior"], "color": "G", "role": TokenCreature(1,1)}))
]
