name = 'Aggressive Urge'
cardnum = 250
expansion = '10E'
type = characteristic('Instant')
supertype = no_characteristic()
subtypes = no_characteristic()
cost = '1G'
color = characteristic(['G'])
text = ['Target creature gets +1/+1 until end of turn.', 'Draw a card.']

out_play_role.abilities = [CastInstantSpell(card, cost,
                                target=[Target(target_types=isCreature), Target(targeting="you")],
                                effects=[AugmentPowerToughness(power=1, toughness=1), DrawCard(1)]),
]