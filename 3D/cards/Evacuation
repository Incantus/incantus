name = 'Evacuation'
cardnum = 83
expansion = '10E'
type = characteristic('Instant')
supertype = no_characteristic()
subtypes = no_characteristic()
cost = '3UU'
color = characteristic(['U'])
text = ["Return all creatures to their owners' hands."]

out_play_role.abilities = [CastInstantSpell(card, cost,
				target=AllPermanentTargets(target_types=isCreature),
				effects=ForEach(ChangeZoneFromPlay(to_zone="hand")))
]