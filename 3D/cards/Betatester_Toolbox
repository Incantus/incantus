  ActivatedAbility(card, cost="0", target=Target(target_types=isCreature), effects=GiveKeyword(protection_from_red, keyword="protection from red")),
  ActivatedAbility(card, cost="0", effects=CreateToken(token_info, number=4)),
  ActivatedAbility(card, cost="0", target=Target(target_types=isCreature), effects=ModifySubtype(add_characteristic("Elf"))),
  ActivatedAbility(card, cost="0", target=Target(target_types=isCreature), effects=AddPowerToughnessCounter()),
