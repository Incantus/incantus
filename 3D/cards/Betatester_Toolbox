cost = '0'
color = no_characteristic()
text = []

out_play_role.abilities = [CastPermanentSpell(card, cost),
  ActivatedAbility(card, cost="0", target=Target(target_types=isPlayer), effects=DealDamage(1), zone="graveyard"),
  ActivatedAbility(card, cost="0", target=Target(targeting="self"), effects=ChangeZoneToPlay(from_zone="library"), zone="library")
]

subrole = Artifact()

in_play_role = Permanent(card, subrole)

