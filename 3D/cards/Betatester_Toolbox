  ActivatedAbility(card, cost="0", target=Target(targeting="you"), effects=MoveCards("library", "hand")),
  ActivatedAbility(card, cost="0", target=Target(targeting="you"), effects=MoveCards("library", "play")),
  ActivatedAbility(card, cost="0", target=Target(target_types=isPermanentType, zone="graveyard"), effects=ChangeZone(from_zone="graveyard", to_zone="play")),
  ActivatedAbility(card, cost="0", target=Target(target_types=isPermanent), effects=ChangeZoneFromPlay(to_zone="hand")),
  ActivatedAbility(card, cost="0", target=Target(targeting="you"), effects=MoveCards(from_zone="graveyard", to_zone="play", number=2)),
