role = TokenCreature(1,1)
haste(role)
token_info = {"name": "Elf Warrior", "type": "Creature", "subtypes": ["Elf", "Warrior", "Druid"], "color": "G", "role": role}

subrole.abilities = [
  ManaAbility(card, cost="0", effects=AddMana(amt="BBBBBWWWWWGGGGGUUUUURRRRR")),
