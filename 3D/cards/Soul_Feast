name = 'Soul Feast'
cardnum = 179
expansion = '10E'
type = characteristic('Sorcery')
supertype = no_characteristic()
subtypes = no_characteristic()
cost = '3BB'
color = characteristic(['B'])
text = ['Target player loses 4 life and you gain 4 life.']

out_play_role.abilities = [CastNonPermanentSpell(card, cost,
                                target=[Target(target_types=isPlayer), Target(targeting="you")],
                                effects=[ChangeLife(-4), ChangeLife(4)])
]
