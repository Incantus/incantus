name = 'Deathmark'
cardnum = 134
expansion = '10E'
type = characteristic('Sorcery')
supertype = no_characteristic()
subtypes = no_characteristic()
cost = 'B'
color = characteristic(['B'])
text = ['Destroy target green or white creature.']

out_play_role.abilities = [CastNonPermanentSpell(card, cost,
				target=Target(target_types=isCreature.with_condition(lambda c: c.color == "G" or c.color == "W")),
				effects=Destroy())
]














