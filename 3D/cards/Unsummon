
name = 'Unsummon'
cardnum = 122
expansion = '10E'
type = characteristic('Instant')
supertype = no_characteristic()
subtypes = no_characteristic()
cost = 'U'
color = characteristic(['U'])
text = ["Return target creature to its owner's hand."]

out_play_role.abilities = [CastInstantSpell(card, cost,
                                target=Target(target_types=isCreature),
                                effects=ChangeZoneFromPlay(to_zone="hand"))
]
