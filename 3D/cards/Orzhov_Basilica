name = 'Orzhov Basilica'
cardnum = 161
expansion = 'Guildpact'
type = characteristic('Land')
supertype = no_characteristic()
subtypes = no_characteristic()
cost = NoCost()
color = no_characteristic()
text = ['Orzhov Basilica comes into play tapped.','When Orzhov Basilica comes into play, return a land you control to its owner\'s hand.','T: Add WB to your mana pool.']

subrole = Land(color)

in_play_role = Permanent(card, subrole)

#################################

in_play_role.tapped = True
subrole.abilities = [ManaAbility(card, cost=TapCost(), effects=AddMana('WB')),]
subrole.triggered_abilities = [TriggeredAbility(card,
  trigger = EnterTrigger("play"),
  match_condition=SelfMatch(card),
  ability=Ability(card, 
    effects=MoveCards(from_zone="play", to_zone="hand", card_types=isLand, required=True)))
]

#################################