name = 'Discombobulate'
cardnum = 81
expansion = '10E'
type = characteristic('Instant')
supertype = characteristic('')
subtypes = characteristic([])
cost = '2UU'
color = characteristic(['U'])
text = ['Counter target spell. Look at the top four cards of your library, then put them back in any order.']

out_play_role.abilities = [CastNonPermanentSpell(card, cost,
                                target=[CounterTarget(target_types=isSpellAbility), Target(targeting="controller")],
                                effects=[CounterAbility(),MoveCards(from_zone="library", to_zone="library", to_position="top", number=4, subset=4)])
]

