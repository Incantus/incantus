name = 'Kiki-Jiki, Mirror Breaker'
cardnum = 175
expansion = 'Champions Of Kamigawa'
type = characteristic('Creature')
supertype = characteristic('Legendary')
subtypes = characteristic(['Goblin', 'Shaman'])
cost = '2RRR'
color = characteristic(['R'])
text = ['Haste', "T Put a token into play that's a copy of target nonlegendary creature you control. That creature token has haste. Sacrifice it at end of turn."]

subrole = Creature(2, 2)

in_play_role = Permanent(card, subrole)

#################################

haste(subrole)

#token_copy = 
subrole.abilities = [ActivatedAbility(card, TapCost,
			target=Target(target_types=isCreature.with_condition(lambda c: not c.supertype == "Legendary")),
			effects=DealDamage(5))] #CreateToken())]
subrole.triggered_abilities = []
subrole.static_abilities = []

#################################
