name = 'Gilded Light'
cardnum = 16
expansion = 'Scourge'
type = characteristic('Instant')
supertype = no_characteristic()
subtypes = no_characteristic()
cost = '1W'
color = characteristic(['W'])
text = ['You gain shroud until end of turn.', 'Cycling 2']

out_play_role.abilities = [CastNonPermanentSpell(card, cost,
				effects=GiveKeyword(shroud, expire=True, keyword='shroud')),
			   Cycling(card, "2")]




