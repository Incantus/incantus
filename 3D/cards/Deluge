name = 'Deluge'
cardnum = 79
expansion = '10E'
type = characteristic('Instant')
supertype = characteristic('')
subtypes = characteristic([])
cost = '2U'
color = characteristic(['U'])
text = ['Tap all creatures without flying.']

out_play_role.abilities = [CastNonPermanentSpell(card, cost, target=AllPermanentTargets(target_types=isCreature.with_condition(lambda c: "flying" not in c.keywords)), effects=ForEach(TapTargetPermanent()))]
