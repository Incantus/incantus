name = 'Assassinate'
cardnum = 128
expansion = '10E'
type = characteristic('Sorcery')
supertype = no_characteristic()
subtypes = no_characteristic()
cost = '2B'
color = characteristic(['B'])
text = ['Destroy target tapped creature.']

out_play_role.abilities = [CastSorcerySpell(card, cost,
				target=Target(target_types=isCreature.with_condition(lambda c: c.tapped)),
				effects=Destroy())]