name = "Eyeblight's Ending"
cardnum = 110
type = characteristic('Instant')
supertype = characteristic('')
subtypes = characteristic(['Elf'])
cost = '2B'
color = characteristic(['B'])
text = ['Destroy target non-Elf creature.']

out_play_role.abilities = [CastNonPermanentSpell(card, cost,
				target=Target(target_types=isCreature.with(lambda c: not c.subtypes == "Elf")),
				effects=Destroy())
]
