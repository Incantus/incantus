name = 'Elvish Promenade'
cardnum = 208
type = characteristic('Sorcery')
supertype = characteristic('')
subtypes = characteristic(['Elf'])
cost = '3G'
color = characteristic(['G'])
text = ['Put a 1/1 green Elf Warrior creature token into play for each Elf you control.']

out_play_role.abilities = [CastNonPermanentSpell(card, cost,
				effects=ForN(number = LazyInt(lambda card=card: len(card.controller.play.get(isPermanent.with(lambda c: c.subtypes == "Elf")))), effect = CreateToken(token_info={"name": "Elf Warrior", "type": "Creature", "subtypes": ["Elf", "Warrior"], "color": "G", "role": TokenCreature(1,1)})))
]
