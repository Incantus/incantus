name = 'Recollect'
cardnum = 289
type = characteristic('Sorcery')
supertype = characteristic('')
subtypes = characteristic([])
cost = '2G'
color = characteristic(['G'])
text = ['Return target card from your graveyard to your hand.']

out_play_role.abilities = [CastNonPermanentSpell(card, cost,
                                target=Target(targeting="controller"),
                                effects=MoveTargetCards(from_zone="graveyard", to_zone="hand", number=1))]
