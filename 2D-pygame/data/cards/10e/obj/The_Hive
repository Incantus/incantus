name = 'The Hive'
cardnum = 324
type = characteristic('Artifact')
supertype = characteristic('')
subtypes = characteristic([])
cost = '5'
color = characteristic(['C'])
text = ['5, T Put a 1/1 Insect artifact creature token with flying named Wasp into play.']

subrole = Artifact()

in_play_role = Permanent(card, subrole)

#################################

token_role = TokenCreature(1,1)
flying(token_role)
token_info = {"name": "Wasp", "type": ["Creature", "Artifact"], "subtypes": ["Insect"], "color": "C", "role": token_role}
subrole.abilities = [ActivatedAbility(card, MultipleCosts([ManaCost("5"), TapCost()]),
                               effects=CreateToken(token_info))
]
subrole.triggered_abilities = []
subrole.static_abilities = []

#################################
