name = 'Rampant Growth'
cardnum = 288
type = characteristic('Sorcery')
supertype = characteristic('')
subtypes = characteristic([])
cost = '1G'
color = characteristic(['G'])
text = ['Search your library for a basic land card and put that card into play tapped. Then shuffle your library.']

out_play_role.abilities = [CastNonPermanentSpell(card, cost,
                                target=Target(targeting="controller"),
                                effects=MoveTargetCards(from_zone="library", to_zone="play", card_types=isLandType.with(lambda l: l.supertype == "Basic"), number=1, func=lambda card: card.tap()))
]
