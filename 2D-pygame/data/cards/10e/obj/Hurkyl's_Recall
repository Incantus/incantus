name = "Hurkyl's Recall"
cardnum = 88
type = characteristic('Instant')
supertype = characteristic('')
subtypes = characteristic([])
cost = '1U'
color = characteristic(['U'])
text = ['Return all artifacts target player owns to his or her hand.']

target=Target(target_types=isPlayer)
out_play_role.abilities = [CastNonPermanentSpell(card, cost,
                                target=[target, AllPermanentTargets(target_types=isArtifact.with(lambda a, t=target: a.owner==t.target))],
                                effects=[NullEffect(), ForEach(ChangeZoneFromPlay(to_zone="hand"))])
]
