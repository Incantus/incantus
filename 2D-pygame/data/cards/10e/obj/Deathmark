name = 'Deathmark'
cardnum = 134
type = characteristic('Sorcery')
supertype = characteristic('')
subtypes = characteristic([])
cost = 'B'
color = characteristic(['B'])
text = ['Destroy target green or white creature.']

out_play_role.abilities = [CastNonPermanentSpell(card, cost,
				target=Target(target_types=isCreature.with(lambda c: c.color == "G" or c.color == "W")),
				effects=Destroy())
]
