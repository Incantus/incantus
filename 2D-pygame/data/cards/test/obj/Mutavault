name = 'Mutavault'
cardnum = 301
type = characteristic('Land')
supertype = characteristic('')
subtypes = characteristic([])
cost = ''
color = characteristic(['C'])
text = ['T: Add 1 to your mana pool', '1: Mutavault becomes a 2/2 creature with all creature types until end of turn. It\'s still a land.']

out_play_role = NoRole(card)
in_play_role = Land(color)

new_role = Creature(2,2)
role_info = {"type": "Creature", "subtypes": all_characteristics(), "color": "C"}
in_play_role.abilities = [ManaAbility(card, cost=TapCost(), effects=AddMana('1')),
                          ActivatedAbility(card, cost="1",
                                target=Target(targeting="self"),
                                effects=AddSubRole(new_role, role_info))
]
in_play_role = Permanent(card, in_play_role)
